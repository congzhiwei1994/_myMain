				#endif
				float2 uv_BaseMap = IN.ase_texcoord3.xy * _BaseMap_ST.xy + _BaseMap_ST.zw;
				float2 uv_NoiseMap = IN.ase_texcoord3.xy * _NoiseMap_ST.xy + _NoiseMap_ST.zw;
				float2 appendResult41 = (float2(_Dir_X , _Dir_Y));
				float4 tex2DNode9 = tex2D( _NoiseMap, ( uv_NoiseMap + ( _TimeParameters.x * _Speed * appendResult41 ) ) );
				float2 appendResult27 = (float2(tex2DNode9.r , tex2DNode9.r));
				float4 tex2DNode6 = tex2D( _BaseMap, ( uv_BaseMap + ( appendResult27 * _DistortIntensity * tex2D( _BaseMap, uv_BaseMap ).a ) ) );
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( _BaseColor * tex2DNode6 ).rgb;
				float Alpha = ( tex2DNode6.a * _Alpha );
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

